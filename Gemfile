source 'https://rubygems.org'

gem 'rails', '3.2.9'

gem 'mysql2'

gem 'name-spotter'

# Gems used only for assets and not required
# in production environments by default.
group :assets do
  gem 'sass-rails',   '~> 3.2.3'
  gem 'coffee-rails', '~> 3.2.1'
  gem 'uglifier', '>= 1.0.3'
end

gem 'jquery-rails'
gem 'uuidtools'
gem 'rsolr'
gem 'ImageResize'

group :development, :test do
  gem 'rspec-rails', '~>2.0'
  gem 'factory_girl_rails'
  gem 'capybara'
  gem 'debugger'
end